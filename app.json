{
  "expo": {
    "name": "BackchannelV2",
    "slug": "backchannelv2",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "backchannelv2",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "Allow access to the camera to capture your front and side profile scans.",
        "NSPhotoLibraryUsageDescription": "Allow access to your photo library to select existing photos for analysis.",
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.yosefwolday.backchannelv2"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "bundleIdentifier": "com.yosefwolday.backchannelv2",
        "projectId": "67d2801c-2564-43a5-8edd-fec2550a5925"
      }
    },
    "owner": "ywolday"
  }
}
